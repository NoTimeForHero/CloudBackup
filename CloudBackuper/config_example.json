{
  "HostingURI": "http://localhost:9000",
  "Cloud": {
    "Provider": "https://s3.selcdn.ru",
    "Login": "999999_UserPrefix",
    "Password": "Password123456",
    "Container": "BucketName",
    "ForcePathStyle": true
  },
  "Logging": {
    "Level": "INFO",
    "WebService": {
      "name": "Remote Logging",
      "url": "http://localhost:3000/example",
      "protocol": "JsonPost",
      "parameters": {
        "api_Key": "e33928aa0c8a3244b8a7013f208e1d24a11dc437",
        "data": "${longdate}",
        "level": "${level:uppercase=true}",
        "logger": "${logger}",
        "message": "${message}"
      }
    },
    "RetryingWrapper": {
      "retryCount": 5,
      "retryDelayMilliseconds": 2000
    }
  },
  "JobRetrying": {
    "MaxRetries": 3,
    "WaitSeconds": 600
  },
  "Jobs": [
    {
      "Name": "Backup system files",
      "Description": null,
      "CronSchedule": "0 0 18 * * ?",
      "Path": "C:\\Windows\\System32",
      "Masks": {
        "Masks": [
          ".ini",
          ".dat"
        ]
      }
    },
    {
      "Name": "User files",
      "Description": null,
      "CronSchedule": "0 0 18 * * ?",
      "Path": "C:\\Users\\user\\Desktop",
      "Masks": {
        "Masks": [
          ".txt",
          ".docx",
          ".xlsx"
        ]
      }
    }
  ]
}