{
  "Cloud": {
    "Provider": "https://s3.selcdn.ru",
    "Login": "999999_UserPrefix",
    "Password": "Password123456",
    "Container": "BucketName",
    "ForcePathStyle": true
  },
  "Logging": {
    "Level": "INFO"
  },
  "Jobs": [
    {
      "Name": "Backup system files",
      "Description": null,
      "CronSchedule": "0 0 18 * * ?",
      "Path": "C:\\Windows\\System32",
      "Masks": {
        "Masks": [
          ".ini",
          ".dat"
        ]
      }
    },
    {
      "Name": "User files",
      "Description": null,
      "CronSchedule": "0 0 18 * * ?",
      "Path": "C:\\Users\\user\\Desktop",
      "Masks": {
        "Masks": [
          ".txt",
          ".docx",
          ".xlsx"
        ]
      }
    }
  ]
}